{
  "name": "fireworks-cool",
  "version": "0.2.0",
  "description": "使用electron构建的一个放烟花的小玩意。",
  "author": "KaiOrange",
  "license": "MIT",
  "repository": "git@github.com:KaiOrange/fireworks-cool.git",
  "main": "main.js",
  "engines": {
    "node": ">=4.0"
  },
  "scripts": {
    "dev": "npm run build && cross-env --NODE_ENV=development electron main.js",
    "start": "npm run build && electron main.js",
    "build": "webpack",
    "buildWin": "electron-packager . fireworks-cool --platform=win32 --arch=x64 --out=./build --asar --ignore=\"(.vscode|.gitignore|.npmignore|README.md|node_modules|build)\"",
    "buildWinia32": "electron-packager . fireworks-cool --platform=win32 --arch=ia32 --out=./build --asar --ignore=\"(.vscode|.gitignore|.npmignore|README.md|node_modules|build)\"",
    "buildMac": "electron-packager . fireworks-cool --platform=darwin --arch=x64 --out=./build --asar --ignore=\"(.vscode|.gitignore|.npmignore|README.md|node_modules|build)\"",
    "buildAll": "npm run build && rimraf ./build && npm run buildWin && npm run buildWinia32 && npm run buildMac"
  },
  "keywords": [
    "fireworks-cool",
    "Electron",
    "fireworks",
    "desktop"
  ],
  "devDependencies": {
    "cross-env": "^5.1.3",
    "css-loader": "^1.0.0",
    "electron-packager": "^13.1.1",
    "style-loader": "^0.21.0",
    "webpack": "^4.16.1",
    "webpack-cli": "^3.1.0",
    "webpack-target-electron-renderer": "^0.4.0"
  },
  "dependencies": {
    "electron": "^1.8.3",
    "jquery": "^3.3.1",
    "mousetrap": "^1.6.1",
    "nouislider": "^11.1.0"
  }
}
