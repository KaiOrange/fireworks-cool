{
  "name": "fireworks-cool",
  "version": "0.0.5",
  "description": "使用electron的一个烟花小程序。",
  "main": "main.js",
  "bin": {
    "fireworks-cool": "index.js"
  },
  "engines": {
    "node": ">=4.0"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=production node index",
    "dev": "cross-env NODE_ENV=development node index",
    "text": "node index --text=HelloWorld",
    "clear": "node index --clear",
    "delete": "node index --delete=1",
    "list": "node index --list",
    "buildWin": "electron-packager . fireworks-cool --platform=win32 --arch=x64 --out=./build --asar --ignore=\"(dist|.vscode|.gitignore|.npmignore|README.md|node_modules)\""
  },
  "keywords": [
    "Electron",
    "fireworks",
    "demo"
  ],
  "author": "KaiOrange",
  "license": "MIT",
  "devDependencies": {
    "cross-env": "^5.1.3",
    "electron-packager": "^12.1.0"
  },
  "dependencies": {
    "electron": "^1.8.3",
    "mousetrap": "^1.6.1",
    "yargs": "^11.0.0"
  }
}
