{
  "name": "fireworks-cool",
  "version": "0.2.1",
  "installVersion": "0.2.0",
  "description": "使用electron构建的一个放烟花的小玩意。",
  "author": "KaiOrange",
  "license": "MIT",
  "repository": "git@github.com:KaiOrange/fireworks-cool.git",
  "bin": {
    "fireworks-cool": "index.js"
  },
  "engines": {
    "node": ">=4.0"
  },
  "scripts": {
    "postinstall": "node install.js",
    "dev": "cross-env NODE_ENV=development node index",
    "start": "cross-env NODE_ENV=production node index",
    "text": "node index --text=HelloWorld",
    "clear": "node index --clear",
    "delete": "node index --delete=1",
    "list": "node index --list"
  },
  "keywords": [
    "fireworks-cool",
    "Electron",
    "fireworks",
    "desktop"
  ],
  "devDependencies": {
    "cross-env": "^5.1.3"
  },
  "dependencies": {
    "extract-zip": "^1.6.7",
    "fs-extra": "^7.0.1",
    "nugget": "^2.0.1",
    "yargs": "^11.0.0"
  }
}
